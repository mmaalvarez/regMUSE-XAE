executor {
    name = 'slurm'
    queueSize = 600
    submitRateLimit = '1 sec'
}

process {
    errorStrategy = 'retry' //{ (task.exitStatus == null || task.exitStatus in [140,143,137,104,134,139,202,247,255]) ? 'retry' : 'ignore'}
    maxRetries = 2
    queue = 'normal_prio' // max 1 day
    cpus = 1
}

trace {
    enabled = true
    trace.overwrite = true
    file = "trace"
}
